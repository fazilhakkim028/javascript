						 controlNav:false,
						 minItems: 1,
					maxItems: 4,
					itemWidth: 193,	
					//itemMargin: 25,	
						touch : true,
						pauseOnHover : true,
				minItems: getGridSize(), 
  			  maxItems: getGridSize(),
  			 start:function(slider){
        		$('.gallery-block .flex-direction-nav li a.flex-prev').css({visibility:'hidden'});
		        slider.find('a').on('click',function(){
		        $('.gallery-block .flex-direction-nav li a.flex-prev').css({visibility:'visible'});
		        });
    		},
			end: function(slider) {
	           $('.last-slide').text(slider.currentSlide);
	           location.href = "small_business_109-aboutus.html.erb";
	         }
			});
		});
		
		 $window.resize(function() {
       var gridSize = getGridSize();
       var flowDir = getSliderDirection();
             flexslider.vars.direction = flowDir;
       flexslider.vars.minItems = gridSize;
        flexslider.vars.maxItems = gridSize;
      });
   }());
