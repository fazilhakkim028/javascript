		<script type="text/javascript">
		$(window).load(function() {
				  var $window = $(window),
        				flexslider;

     
      function getGridSize() {
      return (window.innerWidth < 600) ? 1 : 
              (window.innerWidth < 768) ? 1 : 4;
      }
      		 $(window).load(function() {
			$('.gallery-block').flexslider({
						animation : "slide",
						animationSpeed : 2000,
						animationLoop: true,
						directionNav : true,
